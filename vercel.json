{
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)\\.js",
      "dest": "/$1.js"
    },
    {
      "src": "/(.*)\\.css",
      "dest": "/$1.css"
    },
    {
      "src": "/editor\\.html",
      "dest": "/editor.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
